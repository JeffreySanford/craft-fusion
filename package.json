  "name": "craft-fusion",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "pnpm dlx nx serve craft-web",
    "start:all": "pnpm run build:backend && concurrently \"pnpm run serve:nest\" \"pnpm dlx nx serve craft-go\" \"pnpm dlx nx serve craft-web\"",
    "serve:nest": "cd dist/apps/craft-nest && node src/main.js",
    "serve:go": "cd dist/apps/craft-go && (main.exe || main)",
    "build": "pnpm dlx nx build craft-web",
    "build:prod": "pnpm dlx nx build craft-web --configuration=production",
    "build:backend": "pnpm dlx nx build craft-nest && pnpm dlx nx build craft-go",
    "lint": "pnpm dlx nx run craft-web:lint && pnpm dlx nx run craft-web:stylelint",
    "lint:fix": "pnpm dlx nx run craft-web:lint --fix && pnpm dlx nx run craft-web:stylelint -- --fix",
    "test": "pnpm dlx nx test craft-web",
    "e2e": "pnpm dlx nx e2e craft-web-e2e",
    "test:gotcha": "pnpm dlx nx test gotcha-harness",
    "lint:styles": "pnpm dlx nx run craft-web:stylelint",
    "check:sass": "pnpm exec sass --no-source-map --load-path=apps/craft-web/src/styles apps/craft-web/src/styles.scss:tmp/css/styles.css --style=compressed",
    "check:contrast": "node tools/check-contrast.js",
    "test:a11y": "node tools/run-axe-playwright.js",
    "check:styles": "pnpm run lint:styles && pnpm run check:sass && pnpm run check:contrast"
  },
