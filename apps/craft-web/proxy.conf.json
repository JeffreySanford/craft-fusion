{
  "/api": {
    "target": "http://localhost:3000",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api": "/api"
    }
  },
  "/api-go": {
    "target": "http://localhost:4000",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api-go": "/api-go"
    }
  },
  "/socket.io": {
    "target": "http://localhost:3000",
    "secure": false,
    "ws": true,
    "changeOrigin": true
  }
}
