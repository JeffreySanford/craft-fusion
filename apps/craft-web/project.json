{
  "name": "craft-web",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/craft-web/src",
  "tags": [],
  "schematics": {
    "@schematics/angular:component": {
      "standalone": false
    }
  },
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/craft-web/browser",
        "index": "apps/craft-web/src/index.html",
        "main": "apps/craft-web/src/main.ts",
        "polyfills": "apps/craft-web/src/polyfills.ts",
        "tsConfig": "apps/craft-web/tsconfig.app.json",
        "assets": ["apps/craft-web/src/assets"],
        "jestConfig": "./jest.config.ts",
        "serviceWorker": false,
        "styles": ["@angular/material/prebuilt-themes/azure-blue.css", "apps/craft-web/src/styles/styles.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["apps/craft-web/src/styles"]
        },
        "scripts": [],
        "allowedCommonJsDependencies": ["jszip"]
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/craft-web/src/environments/environment.ts",
              "with": "apps/craft-web/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "aot": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "serviceWorker": true
        },
        "development": {
          "optimization": false,
          "sourceMap": true,
          "namedChunks": true,
          "extractLicenses": false,
          "vendorChunk": true
        }
      }
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "buildTarget": "craft-web:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "craft-web:build:production",
          "proxyConfig": "apps/craft-web/src/proxy.config.prod.json"
        },
        "development": {
          "buildTarget": "craft-web:build:development",
          "proxyConfig": "apps/craft-web/src/proxy.config.json"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "craft-web:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/craft-web/src/**/*.ts", "apps/craft-web/src/**/*.html", "apps/craft-web/src/**/*.scss"]
      }
    },
    "stylelint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx stylelint '**/*.{css,scss,sass,less}' --config .stylelintrc.json"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/craft-web"],
      "options": {
        "jestConfig": "apps/craft-web/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "craft-web:build",
        "port": 4200,
        "staticFilePath": "dist/apps/craft-web/browser",
        "spa": true
      }
    }
  }
}
