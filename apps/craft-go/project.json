{
  "name": "craft-go",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/craft-go/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/go:build",
      "options": {
        "outputPath": "dist/apps/craft-go",
        "main": "apps/craft-go/main.go"
      }
    },
    "serve": {
      "executor": "@nx/go:serve",
      "options": {
        "buildTarget": "craft-go:build"
      }
    }
  }
}
