{
  "name": "craft-go",
  "root": "apps/craft-go",
  "sourceRoot": "apps/craft-go/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o dist/apps/craft-go/main",
        "cwd": "apps/craft-go"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run main.go",
        "cwd": "apps/craft-go"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go test ./...",
        "cwd": "apps/craft-go"
      }
    }
  }
}
